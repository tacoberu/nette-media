{
    "name": "tacoberu/nette-media",
    "description": "On-the-fly generated web images for your Nette app. Very inspired (and copy) from dotBlue (http://dotblue.net).",
    "type": "library",
    "keywords": [
        "routing",
        "images",
        "media",
        "nette"
    ],
    "license": ["MIT"],
    "authors": [
        {
            "name": "Martin Takáč",
            "homepage": "http://martin.takac.name"
        },
        {
            "name": "Vojtěch Dobeš",
            "homepage": "http://github.com/vojtech-dobes"
        },
        {
            "name": "dotBlue",
            "homepage": "http://dotblue.net"
        }
    ],
    "support": {
        "issues": "https://github.com/tacoberu/nette-media/issues",
        "source": "https://github.com/tacoberu/nette-media"
    },
    "require": {
        "php": ">=7.4",
        "nette/application": ">=2.5.0",
        "nette/di": ">=2.5.0",
        "nette/http": ">=2.5.0",
        "latte/latte": ">=2.5.0",
        "nette/utils": ">=2.6.0"
    },
    "autoload": {
        "classmap": ["src/"]
    },
	"scripts": {
		"phpunit": [
			"vendor/bin/phpunit"
		],
		"phpstan": [
			"vendor/bin/phpstan analyse --memory-limit=512M"
		]
	},
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "phpstan/phpstan": "^1.10",
        "tracy/tracy": "^2.10"
    }
}
